<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta charset='utf-8'>
    <meta content='IE=edge' http_equiv='X-UA-Compatible'>
    <meta content='width=device-width, initial-scale=1.0' name='viewport'>
    <meta name='author' content='Designed by Valerie Merchant. Developed by Justin Cook.'>
    <meta name='description' content='Website for our upcoming wedding July 24th, 2015.'>

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title %> - What's Cooking with Us</title>

    <%= stylesheet_link_tag "style" %>
  </head>

  <body class="<%= page_classes %>">

    <%= partial 'layouts/partials/header' %>
    <%= partial 'layouts/partials/nav' %>

    <div class="container content-wrapper">
      <div class="images">
        <%= partial 'layouts/partials/images' %>
      </div><!--/.images -->

      <div class="content">
        <%= yield %>
      </div><!--/.content -->
    </div><!--/.container -->

    <%= partial 'layouts/partials/footer' %>

    <%= javascript_include_tag '//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js' %>
    <%= javascript_include_tag  "all" %>
  </body>
</html>
